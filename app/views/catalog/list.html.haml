.row
  .span12
    %ul.breadcrumb
      %li
        %a{:href => "/"}
          %i.icon-home
        %span.divider /
      %li
        %a{:href => "/catalog/index"}
          Каталог мебели
        %span.divider /
      %li.active Шкафы

.row
  .span4
    .block_content
      .products_list.text_block

        %h4.title
          %span.pull-left
            %i.icon-book
            %strong Название шкафа
        %a{:href => '/catalog/detail', :title => ''}
          %p.obj_img
            %span.sale
              %img{:alt => "", :src => "/assets/sale.png"}

            %img{:src => "https://encrypted-tbn1.gstatic.com/images?q=tbn:ANd9GcT2SVoUu9_ohsYS4DQh8GuXl2f0AyGoEu7gMSTp1MRumZWzJjkkfQ", :alt => '', :width => '100%'}
          %p.obj_info_mini
            %span
              Цена:
            %span
              12 000
          %p.obj_info_mini
            %span
              Размеры:
            %span
              220 x 220 x 220
          %p.obj_info_mini
            %span
              Декор:
            %span
              3 вида обивки
          %p.obj_info_mini
            %span
              Вариация:
            %span
              7 вариантов вида
          .btn-group
            %a.btn{:href => '#', :title => ''} Подробнее
